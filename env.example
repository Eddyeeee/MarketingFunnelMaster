# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/qmoney_funnel"

# Server Configuration
PORT=3000
NODE_ENV=development

# Client Configuration
VITE_API_URL=http://localhost:3000

# Instapage API Configuration
INSTAPAGE_API_KEY=your_instapage_api_key_here
INSTAPAGE_ACCOUNT_ID=your_instapage_account_id_here
INSTAPAGE_BASE_URL=https://api.instapage.com/api/1
INSTAPAGE_TEMPLATE_ID=your_template_page_id_here

# n8n Webhook Configuration
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/lead-capture
N8N_API_KEY=your_n8n_api_key_here

# Service Integration Flags
ENABLE_N8N_INTEGRATION=true
ENABLE_INSTAPAGE_INTEGRATION=true

# Analytics & Tracking
GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
FACEBOOK_PIXEL_ID=your_facebook_pixel_id
TIKTOK_PIXEL_ID=your_tiktok_pixel_id

# Email Service (optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# Security
JWT_SECRET=your_jwt_secret_key_here
SESSION_SECRET=your_session_secret_here

# CORS Configuration
CORS_ORIGIN=http://localhost:5173,http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true 