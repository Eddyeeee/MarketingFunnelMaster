{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build client",
    "build:server": "tsc -p server/tsconfig.json",
    "preview": "vite preview client",
    "start": "NODE_ENV=production node dist/server/index.js",
    "start:prod": "NODE_ENV=production tsx server/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "npm run build:client && npx vite-bundle-analyzer dist/client",
    "lighthouse": "npm run build && npm run start & sleep 5 && npx lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html && pkill -f 'node dist/server/index.js'",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "deps:update": "npx npm-check-updates -u && npm install",
    "deps:check": "npx npm-check-updates",
    "clean": "rm -rf dist coverage .nyc_output",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "release": "npm run test && npm run build && npm run lighthouse && echo 'Ready for deployment!'",
    "deploy:staging": "npm run build && echo 'Deploy to staging...'",
    "deploy:production": "npm run release && echo 'Deploy to production...'",
    "docker:build": "docker build -t q-money-app .",
    "docker:run": "docker run -p 3000:3000 q-money-app",
    "pwa:validate": "npx pwa-asset-generator public/logo.svg public/icons --manifest public/manifest.json",
    "seo:validate": "npx lighthouse-ci autorun",
    "performance:test": "npm run build && npm run start & sleep 5 && npx autocannon http://localhost:3000 -c 10 -d 30 && pkill -f 'node dist/server/index.js'",
    "deploy:qmoney": "./scripts/deploy.sh qmoney",
    "deploy:remotecash": "./scripts/deploy.sh remotecash",
    "deploy:cryptoflow": "./scripts/deploy.sh cryptoflow",
    "deploy:affiliatepro": "./scripts/deploy.sh affiliatepro",
    "deploy:qmoney:vercel": "./scripts/deploy.sh qmoney vercel",
    "deploy:remotecash:vercel": "./scripts/deploy.sh remotecash vercel",
    "deploy:cryptoflow:vercel": "./scripts/deploy.sh cryptoflow vercel",
    "deploy:affiliatepro:vercel": "./scripts/deploy.sh affiliatepro vercel",
    "deploy:qmoney:netlify": "./scripts/deploy.sh qmoney netlify",
    "deploy:remotecash:netlify": "./scripts/deploy.sh remotecash netlify",
    "deploy:cryptoflow:netlify": "./scripts/deploy.sh cryptoflow netlify",
    "deploy:affiliatepro:netlify": "./scripts/deploy.sh affiliatepro netlify",
    "deploy:qmoney:docker": "./scripts/deploy.sh qmoney docker",
    "deploy:remotecash:docker": "./scripts/deploy.sh remotecash docker",
    "deploy:cryptoflow:docker": "./scripts/deploy.sh cryptoflow docker",
    "deploy:affiliatepro:docker": "./scripts/deploy.sh affiliatepro docker",
    "build:qmoney": "PRODUCT_ID=qmoney npm run build",
    "build:remotecash": "PRODUCT_ID=remotecash npm run build",
    "build:cryptoflow": "PRODUCT_ID=cryptoflow npm run build",
    "build:affiliatepro": "PRODUCT_ID=affiliatepro npm run build",
    "dev:qmoney": "PRODUCT_ID=qmoney npm run dev",
    "dev:remotecash": "PRODUCT_ID=remotecash npm run dev",
    "dev:cryptoflow": "PRODUCT_ID=cryptoflow npm run dev",
    "dev:affiliatepro": "PRODUCT_ID=affiliatepro npm run dev"
  },
