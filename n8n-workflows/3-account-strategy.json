      "parameters": {
        "functionCode": "// Parse 3-account content\nconst response = $input.first().json;\nconst content = response.choices[0].message.content;\n\n// Split into account-specific posts\nconst accountPosts = content.split('ACCOUNT:').filter(post => post.trim());\n\nconst formattedPosts = accountPosts.map((post, index) => {\n  const lines = post.trim().split('\\n');\n  const accountName = lines[0].replace('@', '').trim();\n  const postContent = lines.slice(1, -2).join('\\n');\n  const hashtags = lines.slice(-2).join(' ');\n  \n  // Account-specific configuration\n  let accountConfig;\n  if (accountName.includes('digital.wealth.academy')) {\n    accountConfig = {\n      account: 'digital_wealth_academy',\n      pageId: '{{ $json.digitalWealthPageId }}',\n      accessToken: '{{ $json.digitalWealthAccessToken }}',\n      landingPage: '{{ $json.generalLandingPage }}',\n      product: 'general',\n      funnelStage: 'awareness',\n      psychologicalTriggers: {\n        authority: 'ðŸŽ“ Zertifizierte Experten',\n        socialProof: 'ðŸ‘¥ Ãœber 50.000+ Follower',\n        value: 'ðŸ’¡ Kostenlose Tipps' \n      }\n    };\n  } else if (accountName.includes('smartbusiness.flow')) {\n    accountConfig = {\n      account: 'smart_business_flow',\n      pageId: '{{ $json.smartBusinessPageId }}',\n      accessToken: '{{ $json.smartBusinessAccessToken }}',\n      landingPage: '{{ $json.qmoneyLandingPage }}',\n      product: 'qmoney',\n      funnelStage: 'interest',\n      psychologicalTriggers: {\n        scarcity: 'ðŸ”¥ Nur noch 5 PlÃ¤tze verfÃ¼gbar!',\n        urgency: 'âš¡ Angebot lÃ¤uft in 48h ab',\n        socialProof: 'ðŸš€ 1.000+ erfolgreiche Teilnehmer'\n      }\n    };\n  } else if (accountName.includes('passive.income.labs')) {\n    accountConfig = {\n      account: 'passive_income_labs',\n      pageId: '{{ $json.passiveIncomePageId }}',\n      accessToken: '{{ $json.passiveIncomeAccessToken }}',\n      landingPage: '{{ $json.cashmaximusLandingPage }}',\n      product: 'cashmaximus',\n      funnelStage: 'desire',\n      psychologicalTriggers: {\n        authority: 'ðŸ† Premium-Experte',\n        exclusivity: 'ðŸ’Ž Exklusiver Zugang',\n        results: 'ðŸ’° 5.000â‚¬+ monatlich passiv'\n      }\n    };\n  }\n  \n  return {\n    id: `post_${accountConfig.account}_${index + 1}`,\n    account: accountConfig.account,\n    content: postContent,\n    hashtags: hashtags,\n    pageId: accountConfig.pageId,\n    accessToken: accountConfig.accessToken,\n    landingPage: accountConfig.landingPage,\n    product: accountConfig.product,\n    funnelStage: accountConfig.funnelStage,\n    psychologicalTriggers: accountConfig.psychologicalTriggers,\n    scheduledTime: new Date(Date.now() + (index * 2 * 60 * 60 * 1000)).toISOString(),\n    contentType: 'account_specific'\n  };\n});\n\nreturn formattedPosts.map(post => ({ json: post }));"
      },
